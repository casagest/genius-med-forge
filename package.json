{
  "name": "genius-med-forge",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "description": "Advanced Medical AI Platform with Multi-Agent Architecture",
  "author": "GENIUS MedicalCor AI Team",
  "license": "PROPRIETARY",
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "build:dev": "vite build --mode development",
    "build:analyze": "vite build --mode production && npx vite-bundle-visualizer",
    "preview": "vite preview",
    "lint": "eslint . --max-warnings 0",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write \"src/**/*.{ts,tsx,css,md,json}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,css,md,json}\"",
    "typecheck": "tsc --noEmit",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "test:e2e": "cypress open",
    "test:e2e:headless": "cypress run",
    "prepare": "husky",
    "precommit": "lint-staged",
    "validate": "npm run typecheck && npm run lint && npm run test:coverage"
  },
  "dependencies": {
