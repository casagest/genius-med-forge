# Supabase Configuration
VITE_SUPABASE_URL=https://your-project.supabase.co
VITE_SUPABASE_ANON_KEY=your-anon-key-here

# WebSocket Configuration
VITE_WEBSOCKET_URL=ws://localhost:3001
VITE_REALTIME_AGENTS_URL=wss://your-project.supabase.co/functions/v1/realtime-agents

# API Configuration
VITE_API_BASE_URL=https://your-project.supabase.co/functions/v1

# Environment
VITE_ENVIRONMENT=development

# Feature Flags
VITE_ENABLE_VOICE_INTERFACE=true
VITE_ENABLE_3D_VIEWER=true
VITE_ENABLE_REAL_TIME_SYNC=true

# Logging
VITE_LOG_LEVEL=info

# Security
VITE_ALLOWED_ORIGINS=http://localhost:5173,http://localhost:8080
